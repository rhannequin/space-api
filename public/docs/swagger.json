{
  "swagger":"2.0",
  "info":{
    "description":"Space API Swagger documentation",
    "version":"1.0.0",
    "title":"Swagger Space API",
    "termsOfService":"http://swagger.io/terms/",
    "contact":{
      "email":"hello@rhannequ.in"
    },
    "license":{
      "name":"MIT License",
      "url":"https://github.com/rhannequin/space-api/blob/master/LICENSE"
    }
  },
  "host":"",
  "basePath":"/api/v1/",
  "schemes":[
    "https",
    "http"
  ],
  "securityDefinitions":{
    "oauth2":{
      "type":"oauth2",
      "description":"This API uses OAuth 2 with implicit grant flow",
      "authorizationUrl":"http://localhost:3000/oauth/authorize",
      "flow":"implicit",
      "scopes":{
        "planets:read":"Read planets"
      }
    }
  },
  "paths":{
    "/":{
      "get":{
        "summary":"Root endpoint",
        "operationId":"root",
        "produces":[
          "application/json"
        ],
        "responses":{
          "200":{
            "description":"successful operation"
          }
        }
      }
    },
    "/stars":{
      "get":{
        "summary":"Stars endpoint",
        "operationId":"stars",
        "produces":[
          "application/json"
        ],
        "responses":{
          "200":{
            "description":"successful operation"
          }
        },
        "security":[
          {
            "oauth2":[
              "planets:read"
            ]
          }
        ]
      }
    },
    "/planetary_systems":{
      "get":{
        "summary":"Planetary systems endpoint",
        "operationId":"root",
        "produces":[
          "application/json"
        ],
        "responses":{
          "200":{
            "description":"successful operation"
          }
        }
      }
    }
  }
}